				$searchFormByG.on('submit', function(e) {
					e.preventDefault();
					var q = $.trim($('#kyword').val());
					if (q.length) {
						$.ajax({
							method: 'POST',
							url: SYNO_WEB.baseHref + 'api/search/quickSearch',
							data: {
								lang: SYNO_WEB.langCode,
								query: q
							}
						}).always(function() {
							window.location = SYNO_WEB.baseURI + SYNO_WEB.langCode + '/search?q=' + encodeURIComponent(q);
							$searchFormByG[0].reset();
						});
					}
					return false;
				});

				$('#kyword').on('focus', function(e) {
					$searchFormByG.addClass('open');
				});

				$('#kyword').click(function(e) {
					e.stopPropagation();
					$searchFormByG.addClass('open');
				});

				$('#root').click(function() {
					if ($searchFormByG.hasClass('open')) {
						$searchFormByG.removeClass('open');
					}
				});
			});
