	<script type="text/javascript">

	$(function () {
		var ErrorUrl = location.search.replace("?aspxerrorpath=", "");
		if (ErrorUrl.trim() !== "") {
			var query = '';
				if (query.trim() !== "") {
					ErrorUrl = ErrorUrl + "?" + query;
				}
				console.log(ErrorUrl);
				SendGaEvent('ErrorPage', 'ErrorUrl', ErrorUrl);
			}

			var Keyword = $("#keyword");
			var Btn = $(".sub-btn");
			Btn.on('click', function () {
				SearchFunction(Keyword.val());
			});
			Keyword.on('keydown', function (event) {
				if (event.which == 13) {
					SearchFunction(Keyword.val());
					return false;
				}
			});
		});

		$(document).on('click', '#keyword', function () {
			if (typeof jQuery.fn.autocomplete !== 'function')
				return false;
			var ajaxUrl_GetProductName = String.format('/Ajax{0}', '/SearchFunction/GetProductName');
			$("#keyword").off('autocomplete').autocomplete({
				source: function (request, response) {
					$.getJSON(ajaxUrl_GetProductName,
						{ term: encodeURI(request.term.toString()) },
						function(data) {
							response(data);
							fixLtRonRTLForSearch("keyword");
						});
				},
				minLength: 3,						//限制搜尋字元
				//delay: 10,						//控制按鍵後的反應，預設是 400 msec (0.4 秒)
				select: function (event, ui) {
					var url = String.format('{0}/{1}/{2}', '', ui.item.ck, ui.item.url);
					SendGaEvent('Error-Page', 'Searchbar-Select-Model-Url:' + url, location.pathname + location.search);
					window.location.href = url;
				}
			});
		});

		function SearchFunction(strKeyWord) {
			var Keyword = $("#keyword").val();
			if (strKeyWord.length < 3) {
				Keyword.attr("title", "Input 3 Words at least!");
				Keyword.tooltip().tooltip("open");
				setTimeout(function () {
					Keyword.tooltip().tooltip("close");
					Keyword.attr("title", "");
				}, 2000);
				return false;
			} else {
				SendGaEvent('ErrorPage', 'SearchByKeyWord', strKeyWord);
				location.href = "" + "/Search?kw=" + strKeyWord;
			}
		}

	</script>

	
