		var setHoverShowMenu = function(titlObj, bodyName) {


			titlObj.children("a").hover(function() {
				//hover in function

				//將目前的hover時間 初始化
				hoverMenuCurrentTime = 0;

				//將目前的hover interval function 初始化
				clearInterval(setInterValHoverTime);
				setInterValHoverTime = null;

				//每100ms 判斷是否要執行hover效果
				setInterValHoverTime = setInterval(function() {
					//每過100ms hover停留時間+100ms
					hoverMenuCurrentTime = hoverMenuCurrentTime + 100;

					//若hover 停留時間超過設定的最高時間
					if (hoverMenuCurrentTime <= hoverMenuLimitTime) {

						//若hover 停留時間超過設定的最高時間 停止判斷hover 時間
						clearInterval(setInterValHoverTime);
						setInterValHoverTime = null;
						//若hover 停留時間超過設定的最高時間 執行hover效果
						showBodyHover(titlObj);
					}
				}, 100);
			}, function() {
				//hover out function

				//將目前的hover時間 初始化
				hoverMenuCurrentTime = 0;

				//將目前的hover interval function 初始化
				clearInterval(setInterValHoverTime);
				setInterValHoverTime = null;
			});

		}
