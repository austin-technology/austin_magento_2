		var GetLimitSubString = {

			//是否包含中文
			hasZh: function(str){
				for(var i = 0;i < str.length; i++)
				{
					if(str.charCodeAt(i) > 255) //如果是漢字，則字符串長度加2
						return true;
					return false;
				}
			},
			//重新計算長度，中文+2，英文+1
			getlen: function(str){
				var strlen = 0;
				for(var i = 0;i < str.length; i++)
				{
					if(str.charCodeAt(i) > 255) //如果是漢字，則字符串長度加2
						strlen += 2;
					else
						strlen++;
				}
				return strlen;
			},
			//限制中文長度
			limitlTwHk: function(str, len){
				var result = "";
				var strlen = 0;
				for(var i = 0;i < str.length; i++)
				{
					if(str.charCodeAt(i) > 255) //如果是漢字，則字符串長度加2
						strlen += 2;
					else
						strlen++;

					result += str.substr(i,1);

					if(strlen >= len){
						break;
					}
				}
				if (str.length > len) {
					result = result + '..';
				}
				return result;
			},
			//限制中文GIGABYTE全大寫
			limitTwHkGIGABYTE: function(str, ciKeyword) {
				var tempStr = str;
				for (var k = 0; k < ciKeyword.length; k++) {
					if (tempStr.toLowerCase().indexOf(ciKeyword[k]) > -1) {
						//var tempReg=new RegExp(ciKeyword[k],"g");   //加上"g"為全部取代
						//tempStr=tempStr.replace(tempReg ,ciKeyword[k].toUpperCase());  //取代為CI全大寫
						var beginIndex = tempStr.toLowerCase().indexOf(ciKeyword[k]);
						tempStr =
							tempStr.substring(0,beginIndex) //頭
							+tempStr.substring(beginIndex,beginIndex+ciKeyword[k].length).toUpperCase() //符合字串
							+tempStr.substring(beginIndex+ciKeyword[k].length,tempStr.length); //尾
					}
				}
				str = tempStr;
				return str;
			},
			//限制英文長度
			limitEn: function(str, maxNumber, ciKeyword) {
				var split = str.split(" ");
				var totalLong = 0;
				var subString = "";

				for (var i = 0; i < split.length; i++) {
					var getLong = split[i].length;

					//限制GIGABYTE AORUS全大寫
					for (var k = 0; k < ciKeyword.length; k++) {
						if (split[i].indexOf(ciKeyword[i]) > -1) {
							split[i] = ciKeyword[i].toUpperCase();
						}
					}

					totalLong = totalLong + getLong;

					if (totalLong > maxNumber) {
						if (str.length > maxNumber) {
							subString = subString.substring(0, subString.length-1);
							subString = subString + '..';
						}
						return subString;
					} else {
						subString = subString + split[i] + " ";
					}
				}
				return subString;
			}
		}

