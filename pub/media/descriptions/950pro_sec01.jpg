								{
									showPage = 0;
								}
							}
							console.log("showPage : " + showPage);

							$("div.f01 ul.column_ul").css({"transition":"all 0.6s","transform":"translate(" + -(showPage * viewportWidth()) + "px, 0px)"});

							$(".mo_only > li > a").removeClass("active");
