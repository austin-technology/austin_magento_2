div.f01 ul.column_ul > li > a:hover > h3 { text-decoration: underline; text-underline-position: under; }




@media screen and (max-width: 1440px) {

div.f01 { padding-top: 2.7777vw; }

div.f01 hr { width: 2.7777vw; height: 0.3472vw; }

div.f01 h2 { padding: 2.0138vw 0px 1.18055555vw; font-size: 2.2222vw; }

div.f01 ul.column_ul > li > a > h3 { bottom: 1.25vw; font-size: 1.25vw; }

}



@media screen and (max-width: 1024px) {

div.f01 h2 { font-size: 23px; }

div.f01 ul.column_ul > li > a > h3 { font-size: 13px; }

}




@media screen and (max-width: 768px) {

.mo_only { display: block; width: 100%; margin: 4.1666vw 0 6.8055vw; padding-left: 0.8333vw; text-align: center; }

div.feature { max-width: 768px; }

div.f01 hr { width: 5.5555vw; height: 0.6944vw; }

div.f01 { overflow: hidden; padding-top: 6.5277vw; }

div.f01 h2 { padding: 4.4444vw 0px 2.7777vw; font-size: 7.5vw; letter-spacing: 0px; }

div.f01 ul.column_ul { position: relative; width: 100%; min-height: 59.7222vw; overflow: hidden; }

div.f01 ul.column_ul > li { position: relative; top: 0; left: 0; overflow: hidden; float: left; display: inline-block; width: 100%; margin-right: 0; }

div.f01 ul.column_ul > li:last-child { float: left; margin-right: 0; }

div.f01 ul.column_ul > li > a { display: inline-block; width: 100%; height: 100%; }

div.f01 ul.column_ul > li > a > h3 { display: block; position: absolute; bottom: 4.3055vw; width: 100%; font-size: 5.5555vw; font-weight: bold; text-align: center; text-decoration: none; }

div.f01 ul.column_ul > li:first-child > a > h3 {letter-spacing: -0.8px;}

.mo_only > li { display: inline-block; width: 2.5vw; height: 2.3611vw; margin-right: 1.6666vw; }
.mo_only > li > a { box-sizing: border-box; display: inline-block; width: 100%; height: 100%; border-radius: 20px; border: 1px solid #626262; }
.mo_only > li > a.active { background: #626262;  }
}



@media screen and (max-width: 320px) {


